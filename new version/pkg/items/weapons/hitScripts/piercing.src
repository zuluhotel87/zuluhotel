use uo;
use os;
use util;

include ":damage:damage";

program HitScript_Blinding( parms )

        var attacker       := parms[1];
	var defender       := parms[2];
	//var weaponAttacker := parms[3];
        var rawDamage      := parms[4];
 
        ApplyDamageEX( defender, rawDamage, DMG_PHYSICAL, attacker );
        
        return 1;
endprogram
