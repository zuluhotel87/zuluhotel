use uo;
use os;

include ":doors:doors";

program Install()

	return 1;
endprogram

exported function IsDoor( door )

	// object.IsA(POLCLASS_DOOR) is more ideal but this allows for custom doors.
	door := door; //Avoid compiler warnings
 
	return 1;
endfunction

exported function IsOpen( door )

	return CInt( GetObjProperty( door, "Open" ));
endfunction
