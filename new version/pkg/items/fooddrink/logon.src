use uo;
use os;

program Logon_Hunger( mobile )

        var script := Start_Script( ":fooddrink:startup/hunger", mobile );
        if( script.errortext )
                SendSysMessage( mobile, "Error starting script <:fooddrink:startup/hunger> -->"+script.errortext );
                return 0;
        endif

	return 1;
endprogram
