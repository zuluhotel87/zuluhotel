use uo;
use os;

program canRemove( mobile, container, item, move_type )

        //Avoid compiler warnings
        item := item;
        container := container;

	if( mobile.cmdlevel )
		return 1;
	elseif( move_type == MOVETYPE_COREMOVE )
		return 1;
	else
		return 0;
	endif
endprogram
