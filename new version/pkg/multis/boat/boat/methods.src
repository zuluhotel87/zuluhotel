use uo;
use os;
use cfgfile;

program Install()

	return 1;
endprogram

exported function GetShipName( boat )

	var cfg_file := ReadConfigFile( ":*:itemdesc" ); // package itemdesc
	var elem := cfg_file[boat.objtype];
        var name := CStr( elem.ShipObjType );
 
	return name;
endfunction
