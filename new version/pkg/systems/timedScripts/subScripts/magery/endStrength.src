use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndCunning( params )

	var mobile := params[1];
	    params := 0;
	
	var points := CInt( GetObjProperty( mobile, "StrengthMod" ));
	AP_ModifyStatMod( mobile, STRENGTH, -( points ));
 
	EraseObjProperty( mobile, "StrengthMod" );
	
	return 1;
endprogram

