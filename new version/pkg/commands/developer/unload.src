use os;
use uo;

include "include/security";

program textcmd_UnloadAll( who, text )

        if( !AuthorizationCode( who ))
                SendSysMessage( who, "Cancelled." );
                return 0;
        endif

	SendSysMessage( who, "Number of scripts unloaded: "+Unload_Scripts( text ));

	return 1;
endprogram
